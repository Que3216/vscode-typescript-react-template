{
    "name": "typescript-react-template",
    "displayName": "Typescript React Template",
    "description": "Fills out a React Component template when you create a new TSX file",
    "version": "0.0.1",
    "publisher": "q",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "tsx",
        "file",
        "template",
        "typescript",
        "react"
    ],
	"activationEvents": [
        "onLanguage:typescriptreact"
	],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}